{{# partial "titulo" }}	
	<title>Consultoras - Consultorea.me</title>
{{/partial}}
		
{{# partial "contenido" }}	

<div class="container">
	<div class="row">
		<form action="/consultoras" >
			<input type="text" name="filtroNombre">
			<input type="submit">
		</form>
	</div>

	<div class="row">
		<div class="col-md-12">	
		<table class="table table-striped" >
			<hr>
				<td>Nombre</td>
				<td>Cantidad de empleados</td>
			</hr>
		{{#consultoras}}
			<tr>
				<td>{{nombre}}</td>
				<td>{{cantidadEmpleados}}</td>
			</tr>
		{{/consultoras}}				
		</table>
		</div>
	</div>
</div>

{{/partial}}
				
{{> layout.hbs }}